{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wpenginebio/wpenginebio",
	"version": "1.2.1",
	"title": "WP Engine Bio Block",
	"category": "theme",
	"icon": "megaphone",
	"description": "Block to display user data from the WordPress REST API.",
	"supports": {
		"html": false,
		"multiple": false, 
		"color": {
			"background": true,
			"text": true
		}
	},
	"textdomain": "wpenginebio",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"isLoaded": {
			"type": "boolean",
			"default": false
		},
		"user": {
			"type": "object",
			"example": {
				"name": "Thomas Michael",
				"description": "He uses elementor...",
				"user_email": "mendonca.media.group@gmail.com"
			}
		},
		"users": {
			"type": "array"
		},
		"isSelected": {
			"type": "boolean",
			"default": false
		}
	}
}